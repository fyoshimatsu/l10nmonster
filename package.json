{
  "name": "l10nmonster",
  "version": "1.0.0",
  "description": "Continuous localization for the rest of us",
  "main": "l10n.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "./node_modules/.bin/esbuild l10n.js --bundle --outfile=bin/l10n.cjs --platform=node --target=node10.4 --minify",
    "build": "pkg -t node16-macos-x64 --out-path bin bin/l10n.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l10nmonster/l10nmonster.git"
  },
  "keywords": [
    "l10n"
  ],
  "author": "Diego Lagunas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/l10nmonster/l10nmonster/issues"
  },
  "homepage": "https://github.com/l10nmonster/l10nmonster#readme",
  "dependencies": {
    "android-string-resource": "^2.3.4",
    "commander": "^8.3.0",
    "gettext-parser": "^4.0.4",
    "globby": "^12.0.2",
    "knex": "^0.95.13",
    "mysql2": "^2.3.2",
    "pig-latinizer": "^1.0.6",
    "words-count": "^2.0.2",
    "xliff": "^5.7.1"
  },
  "devDependencies": {
    "esbuild": "^0.13.12",
    "pkg": "^5.4.1"
  }
}
